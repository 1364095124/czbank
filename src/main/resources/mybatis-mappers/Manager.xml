<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.relesee.dao.ManagerDao">


    <insert id="insertAdvice">
        INSERT INTO
            managerAdvice(
                managerId,
                content,
                time
            )
        VALUES
            (
                #{managerId},
                #{content},
                NOW()
            )
    </insert>






</mapper>